$
{

	endTile = import 'endTile'
	hcktrck = 0

}

if hcktrck : 0
{
	
	hook tracking
	hcktrck = 1

}
else
{

	endObj = load object endTile
	endObj2 = load object endTile
	obj = load copyobject ?0

	show cursor

	scale obj 0.0 0.0 -2.0
	glue obj endObj 0.0 0.0 1.0
	glue obj endObj2 0.0 0.0 -1.0

	add obj
	add endObj
	add endObj2

	clear tracking
	hcktrck = 0

}